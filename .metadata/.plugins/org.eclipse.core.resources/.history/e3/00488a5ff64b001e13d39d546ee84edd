package entities;

public class Pessoa {
	public double altura;
	public char genero;
	
	public Pessoa(double altura, char genero) {
		this.altura = altura;
		this.genero = genero;
	}
	
	public double getAltura() {
		return altura;
	}
	
	public void setAltura (double altura) {
		this.altura = altura;
	}

}
